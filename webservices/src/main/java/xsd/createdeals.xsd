<?xml version="1.0" encoding="UTF-8"?>  
 <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
			 xmlns="http://webservices/beans/createDeals" 
			 xmlns:account="http://bean.webservices" 
			 targetNamespace="http://webservices/beans/createDeals" 
			 elementFormDefault="qualified">  
 
      <xsd:element name="CreateDealsRequest">   
           <xsd:complexType>  
                <xsd:sequence>  
                     <xsd:element name="deals" type="deals" minOccurs="1" maxOccurs="unbounded"/>  
                </xsd:sequence>  
           </xsd:complexType>  
      </xsd:element>  
      
      <xsd:element name="CreateDealsResponse">  
           <xsd:complexType>  
                <xsd:sequence>  
                     <xsd:element name="status" type="xsd:string"  minOccurs="0" maxOccurs="unbounded"/>
                     <xsd:element name="responseList" type="ResponseList" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>  
           </xsd:complexType>  
      </xsd:element> 
      
      <xsd:complexType name="ResponseList">  
           <xsd:sequence>  
                <xsd:element name="errorMessage" type="xsd:string"  minOccurs="0" maxOccurs="unbounded"/>  
                <xsd:element name="errorDeal" type="deal"  minOccurs="0" maxOccurs="unbounded"/>  
           </xsd:sequence>  
      </xsd:complexType>  
      	
	  <xsd:complexType name="deals">
		<xsd:sequence>
	      <xsd:element name="deal" type="deal" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
	    </xsd:sequence>
	   </xsd:complexType>
	
	  <xsd:complexType name="deal">
	    <xsd:sequence>
	      <xsd:element name="id" type="xsd:int" minOccurs="0"/>
	      <xsd:element name="descrption" type="xsd:string" minOccurs="0"/>
	      <xsd:element name="startDate" type="xsd:dateTime" minOccurs="0"/>
	      <xsd:element name="maturityDate" type="xsd:dateTime" minOccurs="0"/>
	      <xsd:element name="facilityList" type="facility" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
	    </xsd:sequence>
	  </xsd:complexType>
	
	  <xsd:complexType name="facility">
	    <xsd:sequence>
	      <xsd:element name="amount" type="xsd:double" minOccurs="0"/>
	      <xsd:element name="drawDownList" type="drawDown" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
	      <xsd:element name="id" type="xsd:int" minOccurs="0"/>
	      <xsd:element name="maturityDate" type="xsd:dateTime" minOccurs="0"/>
	      <xsd:element name="startDate" type="xsd:dateTime" minOccurs="0"/>
	    </xsd:sequence>
	  </xsd:complexType>
	  
	  <xsd:complexType name="drawDown">
	    <xsd:sequence>
	      <xsd:element name="amount" type="xsd:double" minOccurs="0"/>
	      <xsd:element name="description" type="xsd:string" minOccurs="0"/>
	      <xsd:element name="id" type="xsd:int" minOccurs="0"/>
	    </xsd:sequence>
  </xsd:complexType>
 </xsd:schema>  